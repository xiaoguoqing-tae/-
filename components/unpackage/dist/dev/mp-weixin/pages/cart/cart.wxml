<view><view style="{{'display:'+(token==''?'block':'none')+';'}}"><view class="wz">你还未登录，请前往登录</view><view data-event-opts="{{[['tap',[['gologin',['$event']]]]]}}" class="btn" bindtap="__e">去登录</view></view><checkbox-group style="{{'display:'+(token!==''?'block':'none')+';'}}"><block wx:for="{{infolist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="container"><view data-event-opts="{{[['tap',[['checkboxChange',['$0'],[[['infolist','',index]]]]]]]}}" class="xz" bindtap="__e"><checkbox value="{{[item.price*item.number]}}" checked="{{item.checked}}"></checkbox></view><view class="container2"><image src="{{item.pic}}"></image><view class="container3"><view>{{item.name}}</view><view data-event-opts="{{[['tap',[['del',['$0',index],[[['infolist','',index,'key']]]]]]]}}" style="background:#007AFF;width:60rpx;border-radius:20rpx;" bindtap="__e">删除</view><view class="container4"><view class="price">{{"￥"+item.price}}</view><view class="goods-compute _div"><label data-event-opts="{{[['tap',[['goodsReduce',[index,'$0'],[[['infolist','',index]]]]]]]}}" class="goods-reduce _span" bindtap="__e">-</label><input readonly data-event-opts="{{[['input',[['__set_model',['$0','number','$event',[]],[[['infolist','',index]]]]]]]}}" value="{{item.number}}" bindinput="__e"/><label data-event-opts="{{[['tap',[['goodsAdd',[index,'$0'],[[['infolist','',index]]]]]]]}}" class="goods-add _span" bindtap="__e">+</label></view></view></view></view></view></block></checkbox-group><view style="height:130rpx;"></view><view class="cart_pay"><view class="left"></view><view class="right"><view class="total">{{"合计：￥"+totalprice}}</view><button data-event-opts="{{[['tap',[['topaymoney',['$event']]]]]}}" bindtap="__e">结算</button></view></view></view>