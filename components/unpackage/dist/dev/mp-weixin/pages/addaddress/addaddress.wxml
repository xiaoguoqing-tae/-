<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><text data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.addressName+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">所属省份编码</text><input class="input" type="text" placeholder="所属省份编码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','procode','$event',[]],['addressData']]]]]}}" value="{{addressData.procode}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">所属城市编码</text><input class="input" type="text" placeholder="所属城市编码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','citycode','$event',[]],['addressData']]]]]}}" value="{{addressData.citycode}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>